(lambda x,y,z: x*y*z)(1,2,3)
(lambda list: lista == 0)([])
(lambda n,list: len(list) > n)(3,[1,2,3,4])
(lambda x,n: x**(1.0/n))(3,5)
(lambda x,y: x&y)({3,4,5},{1,2,3})

### map ###

	map(lambda x: x+5, [1,2,3,4,5])
	map(lambda x: x[0], ['HOla', 'Como', 'Estas'])

### filter ###

	dic = {1:'oso', 2:'hola', 3 'ama'}
	filter(lambda value: value == value[::-1], dic.values())

### reduce ###

	list = [1,2,3,4,5,6,7,8,9]
	reduce(lambda x,y: x+y, list)

	
